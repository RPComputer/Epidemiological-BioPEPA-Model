% Bio-PEPA model of Na Cl

%          k1
% Na + Cl <--> Na_p:Cl_m
%          k1m

% ODEs:
%
% dNa/dt        = -k1 Na Cl + k1m Na_p:Cl_m
% dCl/dt        = -k1 Na Cl + k1m Na_p:Cl_m
% dNa_p:Cl_m/dt =  k1 Na Cl - k1m Na_p:Cl_m
%
% r1  = k1 Na Cl
% r1m = k1m Na_p:Cl_m


r1 = [k1 * Na * Cl];
r2 = [K1m * Na_p:Cl_m];

Na          = r1<< + r2>> ;
Cl          = r1<< + r2>> ;
Na_p:Cl_m   = r1>> + r2<< ;

(Na <> (Cl <> Na_p:Cl_m))
